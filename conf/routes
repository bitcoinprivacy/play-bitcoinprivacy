GET  /favicon.ico   controllers.Assets.at(path="/public", file="img/bitcoin.png")

GET  /assets/*file        controllers.Assets.at(path="/public", file)

GET     /                           controllers.Application.explorer


POST    /search                       controllers.Application.search

GET    /wallet/:address                      controllers.Application.wallet(address: String)

GET    /richlist             controllers.Application.richList

GET    /stats             controllers.Application.stats

GET    /distribution/:value      controllers.Application.distribution(value)

POST   /distribution      controllers.Application.distributionPost

GET    /faq                     controllers.Application.faq

GET    /explorer               controllers.Application.explorer

GET   /block/:height          controllers.Application.block(height: String) 

GET   /transaction/:hash      controllers.Application.transaction(hash: String)

GET    /address/:hash         controllers.Application.address(hash: String)